{
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/(.*)$",
    "^[./]"
  ],
  "singleQuote": true,
  "semi": false,
  "tabWidth": 2,
  "bracketSpacing": true,
  "trailingComma": "es5",
  "bracketSameLine": true,
  "useTabs": false,
  "printWidth": 120,
  "arrowParens": "always",
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true,
  "importOrderSeparation": true
}
